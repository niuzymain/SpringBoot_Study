<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nzy.miaosha.dao.UserMapper">
<select id="selectByPrimaryKey" resultType="com.nzy.miaosha.entity.User">
  select user_id,user_name,phone,register_mode,thirdpay_id
  from user
  where user_id=#{userid}
</select>
</mapper>